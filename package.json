{
  "name": "music",
  "version": "1.0.0",
  "description": "learn-vuejs",
  "main": "webpack.config.js",
  "jshintConfig": {
    "esversion": 6,
    "strict": false,
    "predef": [
      "setTimeout"
    ],
    "node": false
  },
  "scripts": {
    "test": "nodemon public/backApp.js",
    "data": "node backend/DataWriterTool/index.js",
